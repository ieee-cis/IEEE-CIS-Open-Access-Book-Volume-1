<?xml version="1.0" encoding="UTF-8" ?>

<!--
     17/06/2015

     Grammar for PFSP Total Completion Time
     without time related termination and optimized for simple algorithms
     
     16/04/2018

     Removed slack and random and added bsch.
     
     30/04/2018

     Added csinsert neighborhood, msnoimprov and swpinc and als local searches

     21/08/2018

     Added ris localsearch and mrsilsp perturbation
 -->

<gr:grammar xmlns:gr="grammar">
    <gr:derivations xmlns:pfsp="pfsp_automatic_generation">
		
	 <pfsp:steps type="int" min="1" max="300" stepIfEnumerated="1"/>
	 <pfsp:sp_impact type="int" min="1" max="20" stepIfEnumerated="1"/>
         <pfsp:explore_steps type="int" min="1" max="20" stepIfEnumerated="1"/>
	 <pfsp:s_acc_temp type="real" min="0.1" max="5" stepIfEnumerated="0.1"/>
	 <pfsp:metrotemp type="real" min="0.1" max="30" stepIfEnumerated="1"/>
	 <pfsp:tabutenure_size type="int" min="1" max="20" stepIfEnumerated="1"/>
	 <pfsp:random_moves type="int" min="1" max="10" stepIfEnumerated="1"/>
	 <pfsp:start_temp type="real" min="1" max="5.0" stepIfEnumerated="0.1"/>
	 <pfsp:end_temp type="real" min="0" max="1.0" stepIfEnumerated="0.1"/>
       	 <pfsp:temp_ratio type="real" min="0.01" max="0.1" stepIfEnumerated="0.01"/>
       	 <pfsp:metro_alpha type="real" min="0.1" max="1" stepIfEnumeraterd="0.1"/>
	 <pfsp:tempiterations type="int" min="1" max="500" stepIfEnumerated="1"/>	
	 <pfsp:tmiig_tabu_size type="int" min="0" max="20" stepIfEnumerated="1" />

<pfsp:always_acc_param>
	<![CDATA[intensify ]]>
    <or/> 
   <![CDATA[diversify ]]>
</pfsp:always_acc_param>
	
<pfsp:initial_solution>
   <![CDATA[bsch 1 ]]>
   <or/>
   <![CDATA[nrz ]]>
  <or/>
  <![CDATA[nrz2 ]]>
 <or/>
  <![CDATA[mneh ]]>
 <or/>
  <![CDATA[neh ]]>
 <or/>
  <![CDATA[nehff ]]>
  <or/>
  <![CDATA[lrnm ]]>
</pfsp:initial_solution>

<pfsp:termination>
   <![CDATA[locmin ]]>
   <or/>
    <![CDATA[maxstep ]]><pfsp:steps/><![CDATA[ ]]>
   <or/>
   <![CDATA[msnoimprov ]]>
</pfsp:termination>

<pfsp:neighborhood>
	   <![CDATA[exchange ]]>
	<or/>
	   <![CDATA[transpose ]]>
	<or/>
	   <![CDATA[ntctinsert ]]>
	<or/>
	   <![CDATA[finsert ]]>
	<or/>
	   <![CDATA[binsert ]]>
	<or/>
	  <![CDATA[tinsert ]]>
	<or/>
	   <![CDATA[nrztctinsert ]]>
	<or/>
	   <![CDATA[csinsert ]]>
	</pfsp:neighborhood>
	
	<pfsp:pertubation>
		<![CDATA[igio ]]><pfsp:sp_impact/><![CDATA[ ]]>
	  	<or/>
		<![CDATA[igoper ]]><pfsp:sp_impact/><![CDATA[ ]]>
		<or/>
		<![CDATA[rndmv ]]><pfsp:neighborhood/><pfsp:random_moves/><![CDATA[ ]]>
		<or/>
		<![CDATA[noper ]]>
		<or/>
		<![CDATA[tmiigper ]]><pfsp:sp_impact/><![CDATA[ ]]><pfsp:tmiig_tabu_size/><![CDATA[ ]]>
		<or/>
		<![CDATA[igols ]]><pfsp:sp_impact/><![CDATA[ ]]><pfsp:localsearch/>
		<or/>
		<![CDATA[mrsilsp ]]><pfsp:random_moves/><![CDATA[ rndmv ]]><pfsp:neighborhood/><pfsp:random_moves/><![CDATA[ ]]>
	</pfsp:pertubation>

	<pfsp:acceptance>
		<![CDATA[rsacc ]]><pfsp:s_acc_temp/><![CDATA[ ]]>
		<or/>
		<![CDATA[explore ]]><pfsp:explore_steps/><![CDATA[ ]]>
		<or/>
		<![CDATA[improve ]]>
		<or/>
		<![CDATA[always ]]><pfsp:always_acc_param/>
		<or/>
		<![CDATA[pmetro ]]><pfsp:start_temp/><![CDATA[ ]]><pfsp:end_temp/><![CDATA[ ]]><pfsp:temp_ratio/><![CDATA[ ]]><pfsp:tempiterations/><![CDATA[ ]]>
		<or/>
		<![CDATA[saacc ]]><pfsp:start_temp/><![CDATA[ ]]><pfsp:end_temp/><![CDATA[ ]]><pfsp:temp_ratio/><![CDATA[ ]]><pfsp:tempiterations/><![CDATA[ ]]><pfsp:metro_alpha/><![CDATA[ ]]>
	</pfsp:acceptance>	

	<pfsp:tabutenure>
	     <![CDATA[move ]]><pfsp:tabutenure_size/><![CDATA[ ]]>
	     <or/>
	     <![CDATA[hash ]]><pfsp:tabutenure_size/><![CDATA[ ]]>
	     <or/>
	     <![CDATA[solution ]]><pfsp:tabutenure_size/><![CDATA[ ]]>
	</pfsp:tabutenure>

        <pfsp:als> 
	  <![CDATA[als ]]>
	  <pfsp:initial_solution/>
          <pfsp:localsearch/>
          <pfsp:localsearch/>
	</pfsp:als>

        <pfsp:ris>
          <![CDATA[ris ]]>
          <pfsp:initial_solution/>
        </pfsp:ris>        

	<pfsp:swpinc>
	  <![CDATA[swpinc ]]>
	  <pfsp:initial_solution/>
	</pfsp:swpinc>
	
	<pfsp:swpincp>
	  <![CDATA[swpinc random ]]>
	</pfsp:swpincp>

	<pfsp:first>
           <![CDATA[first ]]>
		  <pfsp:initial_solution/>
          	  <pfsp:termination/>
  		  <pfsp:neighborhood/>
	</pfsp:first>

	<pfsp:best>
           <![CDATA[best ]]>
           <pfsp:initial_solution/>
           <pfsp:termination/>
	   <pfsp:neighborhood/>
	</pfsp:best>

	<pfsp:ils>
		<![CDATA[ils ]]>
		<pfsp:ilsalgos/>
		<pfsp:termination/>
		<pfsp:pertubation/>
		<pfsp:acceptance/>
	</pfsp:ils>
	
	<pfsp:ails>
		<![CDATA[ils ]]>
		<pfsp:ilsalgos/>
	        <![CDATA[true ]]>
		<pfsp:pertubation/>
		<pfsp:acceptance/>
	</pfsp:ails>
        
	<pfsp:gls>
           <![CDATA[ils ]]>
		<pfsp:ils/>
	        <![CDATA[true ]]>
		<pfsp:pertubation/>
		<pfsp:acceptance/>
	</pfsp:gls>
	
	<pfsp:tabu>
           <![CDATA[tabu best ]]>
              	<pfsp:initial_solution/>
        	<pfsp:termination/>
		<pfsp:neighborhood/>
		<pfsp:tabutenure/>
	</pfsp:tabu>

	<pfsp:stabu>
           <![CDATA[tabu best ]]>
        	<pfsp:initial_solution/>
	        <![CDATA[true ]]>
		<pfsp:neighborhood/>
		<pfsp:tabutenure/>
	</pfsp:stabu>

	<pfsp:tabuf>
           <![CDATA[tabu first ]]>
              	<pfsp:initial_solution/>
        	<pfsp:termination/>
		<pfsp:neighborhood/>
		<pfsp:tabutenure/>
	</pfsp:tabuf>

	<pfsp:stabuf>
           <![CDATA[tabu first ]]>
        	<pfsp:initial_solution/>
	        <![CDATA[true ]]>
		<pfsp:neighborhood/>
		<pfsp:tabutenure/>
	</pfsp:stabuf>

	<pfsp:vnd>
           <![CDATA[vnd ]]>
		<pfsp:vnds/>
		<pfsp:initial_solution/>
		<pfsp:termination/>
		<pfsp:neighborhood/>
		<pfsp:neighborhood/>
		<pfsp:neighborhood/>
	</pfsp:vnd>

	<pfsp:vnds>
		<![CDATA[first ]]>
		<or/>
		<![CDATA[best ]]>
	</pfsp:vnds>

	<pfsp:localsearch>
		<pfsp:firstp/>
		<or/>
		<pfsp:bestp/>
		<or/>
		<pfsp:swpincp/>
	</pfsp:localsearch>	

	<pfsp:firstp>
           <![CDATA[first random ]]>
          	  <pfsp:termination/>
  		  <pfsp:neighborhood/>
	</pfsp:firstp>

	<pfsp:bestp>
           <![CDATA[best random ]]>
           <pfsp:termination/>
	   <pfsp:neighborhood/>
	</pfsp:bestp>
	
	<pfsp:ilsalgos>
		<pfsp:first/>
		<or/>
		<pfsp:best/>
		<or/>
		<pfsp:ils/>
		<or/>
		<pfsp:tabu/>
		<or/>
		<pfsp:vnd/>
		<or/>
		<pfsp:tabuf/>
		<or/>
		<pfsp:als/>
		<or/>
		<pfsp:swpinc/>
		<or/>
		<pfsp:ris/>
	</pfsp:ilsalgos>
	
	<pfsp:algos>
		<pfsp:ails/>
		<or/>
		<pfsp:stabu/>
		<or/>
		<pfsp:gls/>
		<or/>
		<pfsp:stabuf/>
	</pfsp:algos>


        <pfsp:main_algorithm output="params">
                <![CDATA[PFSP_TCT ]]>
        	<pfsp:algos/>     
        </pfsp:main_algorithm>

    </gr:derivations>
</gr:grammar>
